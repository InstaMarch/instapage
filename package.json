{
  "name": "instapage",
  "version": "0.0.1",
  "description": "Super simple static site generator.",
  "main": "index.js",
  "config": {
    "lintcmd": "./node_modules/.bin/jslint ./*.js ./tests/*.js"
  },
  "scripts": {
    "test": "nyc tape ./tests/test.js",
    "instalint": "$npm_package_config_lintcmd",
    "server:launch": "open ",
    "build:lab": "cd lab && node build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InstaMarch/instapage.git"
  },
  "keywords": [
    "static",
    "node",
    "js"
  ],
  "contributors": ["Mike Ricos", "Ben Pardo"],

 "licenses": [{
    "type": "MIT",
    "url": "https://www.opensource.org/licenses/mit-license.php"
  }],

  "bugs": {
    "url": "https://github.com/InstaMarch/instapage/issues"
  },
  "homepage": "https://github.com/InstaMarch/instapage#readme",
  "devDependencies": {
    "jslint-node": "^1.2.4",
    "nyc": "^11.0.2",
    "tape": "^4.6.3"
  }
}
